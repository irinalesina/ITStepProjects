<Application x:Class="TaskManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:TaskManager.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:BytesToKilobytesStringConverter x:Key="bytesToKilobytesStringConverter"/>
        <converters:ProcessToInfoConverter x:Key="processToInfoConverter"/>
        <converters:ProcessToCurrentProcessName x:Key="processToCurrentProcessName"/>
    </Application.Resources>
</Application>
