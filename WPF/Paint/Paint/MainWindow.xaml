<Window Name="mainWimdow" x:Class="Paint.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" WindowState="Maximized" Loaded="Window_Loaded" MouseDown="mainWimdow_MouseDown">
    <Grid Name="mainGrid" MouseUp="mainGrid_MouseUp">
        <ComboBox Name="comboBoxElements" HorizontalAlignment="Left" VerticalAlignment="Top" Width="150" Margin="163,0,0,0" Height="31" SelectionChanged="comboBoxElements_SelectionChanged"/>
        <Label Name="lableElements" Content="Select element" HorizontalAlignment="Left" VerticalAlignment="Top" Width="158" Height="31" FontFamily="SketchFlow Print" FontSize="20"/>
    </Grid>
</Window>
